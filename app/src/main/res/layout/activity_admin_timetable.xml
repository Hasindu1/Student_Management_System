<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminTimeTable">

    <TextView
        android:id="@+id/selectId_3001"
        android:layout_width="110dp"
        android:layout_height="35dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="24dp"
        android:text="@string/select_id3001"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/selectIdSpinner_3001"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/selectIdSpinner_3001"
        android:layout_width="181dp"
        android:layout_height="40dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/viewTimetable_2"
        android:layout_width="73dp"
        android:layout_height="47dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="52dp"
        android:text="@string/viewTimetable_admin"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/selectIdSpinner_3001" />

    <TextView
        android:id="@+id/timetableId_2"
        android:layout_width="160dp"
        android:layout_height="39dp"
        android:layout_marginTop="104dp"
        android:layout_marginEnd="32dp"
        android:text="@string/time_table_id_admin"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/adminTimetableId"
        app:layout_constraintTop_toBottomOf="@+id/selectIdSpinner_3001" />

    <EditText
        android:id="@+id/adminTimetableId"
        android:layout_width="155dp"
        android:layout_height="40dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="44dp"
        android:ems="10"
        android:inputType="number"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewTimetable_2" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="76dp"
        android:text="@string/teacher_admin"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/teacher_3001"
        app:layout_constraintTop_toBottomOf="@+id/timetableId_2" />

    <EditText
        android:id="@+id/teacher_3001"
        android:layout_width="155dp"
        android:layout_height="46dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="48dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/adminTimetableId" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="92dp"
        android:text="@string/venue_admin"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/venue_3001"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <EditText
        android:id="@+id/venue_3001"
        android:layout_width="156dp"
        android:layout_height="44dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="48dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/teacher_3001" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="116dp"
        android:text="@string/day_admin"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/day_3001"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <EditText
        android:id="@+id/day_3001"
        android:layout_width="154dp"
        android:layout_height="43dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="52dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/venue_3001" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="100dp"
        android:text="@string/time_admin"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/time_3001"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <EditText
        android:id="@+id/time_3001"
        android:layout_width="155dp"
        android:layout_height="44dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="52dp"
        android:ems="10"
        android:inputType="time"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/day_3001" />

    <Button
        android:id="@+id/add_data3000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="73dp"
        android:layout_marginEnd="27dp"
        android:text="@string/add_timetable_data"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/update_data3000"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <Button
        android:id="@+id/update_data3000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="82dp"
        android:layout_marginEnd="24dp"
        android:text="@string/update_timetable_data"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/delete_data3000"
        app:layout_constraintTop_toBottomOf="@+id/time_3001" />

    <Button
        android:id="@+id/delete_data3000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="82dp"
        android:layout_marginEnd="48dp"
        android:text="@string/delete_timetable_data"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/time_3001" />

</androidx.constraintlayout.widget.ConstraintLayout>